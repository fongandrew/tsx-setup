{
  "compilerOptions": {
    "target": "es5",
    "sourceMap": true,
    "importHelpers": true,
    "noEmitHelpers": true,
    "forceConsistentCasingInFileNames": true,
    "alwaysStrict": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "allowUnreachableCode": false,
    "allowUnusedLabels": false,
    "noFallthroughCasesInSwitch": true,
    "strictNullChecks": true,
    "module": "es6",
    "moduleResolution": "node",
    "jsx": "react",
    "lib": ["dom", "es2017"],
    "paths": {
      "@/*": ["./src/*"],
      "*": ["./typings/*"]
    },
    "baseUrl": "."
  }
}